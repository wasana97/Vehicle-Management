<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>OOP Vehicle Web App</display-name>
 
  <servlet>
    <servlet-name>AddVehicleServlet</servlet-name>
    <servlet-class>com.oop.servlet.AddVehicleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateVehicleServlet</servlet-name>
    <servlet-class>com.oop.servlet.UpdateVehicleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteVehicleServlet</servlet-name>
    <servlet-class>com.oop.servlet.DeleteVehicleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetVehicleServlet</servlet-name>
    <servlet-class>com.oop.servlet.GetVehicleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListVehicleServlet</servlet-name>
    <servlet-class>com.oop.servlet.ListVehicleServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>AddVehicleServlet</servlet-name>
    <url-pattern>/AddVehicleServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateVehicleServlet</servlet-name>
    <url-pattern>/UpdateVehicleServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteVehicleServlet</servlet-name>
    <url-pattern>/DeleteVehicleServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetVehicleServlet</servlet-name>
    <url-pattern>/GetVehicleServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>ListVehicleServlet</servlet-name>
    <url-pattern>/ListVehicleServlet</url-pattern>
  </servlet-mapping>
 
 
  <welcome-file-list>
    <welcome-file>homeView.html</welcome-file>
    <welcome-file>homeView.htm</welcome-file>
    <welcome-file>homeView.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

</web-app>